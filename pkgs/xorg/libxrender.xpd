name: libxrender
description: X Rendering Extension client library
version: 0.9.8

build-dependencies:
  - autotools-build
  - tl:c-compiler
  - tl:linker
  - pkg-config

dependencies:
  - tl:libc
  - libx11
  - renderproto

files:
  md5-2bd9a15fcf64d216e63b8d129e4f1f1c:
    url: http://xorg.freedesktop.org/releases/individual/lib/libXrender-0.9.8.tar.bz2

configure:
  ./configure --prefix=%(prefix)s

build:
  make -j%(jobs)s

install:
  make -j%(jobs)s install
