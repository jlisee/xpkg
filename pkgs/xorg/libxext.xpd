name: libxext
description: X11 miscellaneous extension library
version: 1.3.3

build-dependencies:
  - autotools-build
  - tl:c-compiler
  - tl:linker

dependencies:
  - tl:libc
  - libx11
  - xextproto

files:
  md5-52df7c4c1f0badd9f82ab124fb32eb97:
    url: http://xorg.freedesktop.org/releases/individual/lib/libXext-1.3.3.tar.bz2

configure:
  ./configure --prefix=%(prefix)s --disable-static

build:
  make -j%(jobs)s

install:
  make -j%(jobs)s install
