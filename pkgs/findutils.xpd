name: findutils
description: utilities for finding files--find, xargs
version: 4.2.2

build-dependencies:
  - autotools-build
  - tl:c-compiler
  - tl:linker

dependencies:
  - tl:libc

files:
  md5-351cc4adb07d54877fa15f75fb77d39f:
    url: http://ftp.gnu.org/pub/gnu/findutils/findutils-4.4.2.tar.gz

configure:
  ./configure --prefix=%(prefix)s

build:
  make -j%(jobs)s

install:
  make install