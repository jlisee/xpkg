name: expat
description: XML parsing C library
version: 2.1.0

build-dependencies:
  - autotools-build
  - tl:c-compiler
  - tl:linker

dependencies:
  - tl:libc

files:
  md5-dd7dab7a5fea97d2a6a43f511449b7cd:
    url: http://downloads.sourceforge.net/project/expat/expat/2.1.0/expat-2.1.0.tar.gz

configure:
  ./configure --prefix=%(prefix)s

build:
  make -j%(jobs)s

install:
  make install
