name: libmpfr
description: GNU MPFR Library (C floating point library)
version: 3.1.2

# Circular dep (this lib is xneeded to build gcc)
build-dependencies:
  - autotools-build
  - tl:c-compiler
  - tl:linker

dependencies:
 - tl:libc
 - libgmp==5.1.2

files:
  md5-ee2c3ac63bf0c2359bf08fc3ee094c19:
    url: http://www.mpfr.org/mpfr-current/mpfr-3.1.2.tar.bz2

configure:
  ./configure --prefix=%(prefix)s

build:
  make -j%(jobs)s

install:
  make install
